$('#status').replaceWith("<a id='status' data-toggle='tooltip' title='Job Run in Progress'><img src='#{escape_javascript image_url('executing.gif')}' /></a>");

- @exec_id.each do |ex|
	$("#exec_#{ex}").replaceWith("<span id='exec_#{ex}'><a data-toggle='tooltip' title='Executing Job'><img src='#{escape_javascript image_url('step.gif')}' /></a></span>");